{
  "expo": {
    "name": "Event Booking",
    "slug": "event-booking",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventbooking.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.eventbooking.app"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "event-booking",
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "newArchEnabled": true
  }
}
